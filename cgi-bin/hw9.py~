#!/usr/bin/python3

# Import modules for CGI handling 
import cgi 


print ("Content-type:text/html\r\n\r\n")
print ("<html>")
print ("<head>")
print ("<title>HW 9</title>")
print ("</head>")
print ("<body>")
form = cgi.FieldStorage()
value=form.getvalue("value")
value += "hi"
print ("<p> " + value + "</p>")
print ("</body>")
print ("</html>")
